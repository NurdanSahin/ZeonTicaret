
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Zeon</title>
    <meta name="description" content="Here goes description" />
    <meta name="author" content="author name" />
    <link rel="shortcut icon" href="/Content/Zeon/images/favicon.ico" type="image/x-icon" />
    <!-- ======================================================================
                                Mobile Specific Meta
    ======================================================================= -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <!-- ======================================================================
                                Style CSS + Google Fonts
    =======================================================================
    <link href="/Content/Zeon/http://fonts.googleapis.com/css?family=Open+Sans:300,400italic,700italic,400,600,700,800" rel="stylesheet" type="text/css"> -->
    <link rel="stylesheet" href="/Content/Zeon/css/bootstrap.css" />
    <link rel="stylesheet" href="/Content/Zeon/css/style.css" />
    @RenderSection("head",false)
</head>

<body class="home-blog-show">
   
    <div class="header">
        <div class="container">
            <div class="header-top-info">
                <ul class="header-top-socials">
                    <li><a href="/Content/Zeon/#"><i class="icon-160" title="161"></i></a></li>
                    <li><a href="/Content/Zeon/#"><i class="icon-138" title="161"></i></a></li>
                    <li><a href="/Content/Zeon/#"><i class="icon-106" title="161"></i></a></li>
                    <li><a href="/Content/Zeon/#"><i class="icon-169" title="161"></i></a></li>
                    <li><a href="/Content/Zeon/#"><i class="icon-163" title="161"></i></a></li>
                </ul>

            </div>
            <div class="header-middle-info">
                <div class="col-md-4">
                    <div class="logo">
                        <a href="/Content/Zeon/index.html">
                            <img src="/Content/Zeon/images/elements/logo.png" alt="site logo" />
                        </a>
                    </div>
                </div>
                <div class="col-md-8">
                    <ul class="header-middle-account">
                        <li><a href="/Content/Zeon//Kullanici/Hesabim"><i class="icon-330" title="My account"></i> Hesabım</a></li>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li><a href="/Content/Zeon//Kullanici/CikisYap"><i class="icon-351" title="Login"></i> Çıkış Yap</a></li>
                        }

                        else {
                            <li><a href="/Content/Zeon//Kullanici/GirisYap"><i class="icon-351" title="Login"></i> Giriş Yap</a></li>
                        }
                    </ul>
                </div>
                <div class="clear"></div>
            </div>
            <div class="menu">
                <div class="search-cart">
                    <form class="search">
                        <input type="text" class="search-line" placeholder="Ara" name="serch" />
                        <input type="submit" value="" class="search-button" />
                    </form>
            

                    @{
                        Html.RenderAction("MiniSepetWidget", "Home");
                    }

                    <div class="clear"></div>
                </div>
                <div class="repsonsive-menu"><i class="icon-406" title="406"></i> Menu</div>
                <ul>
                    <li class="menu-item-has-children active">
                        <a href="/Content/Zeon//Home/Index">Anasayfa</a>



                    <li><a href="/Content/Zeon//Home/Kategoriler">Kategoriler</a></li>


                    <li class="menu-item-has-children">
                        <a href="/Content/Zeon/#">Sayfalar</a>
                        <ul>

                            <li><a href="/Content/Zeon//Home/Hakkimizda">Hakkmızda</a></li>

                            <li><a href="/Content/Zeon//Kullanici/KayitOl">Kayıt Ol</a></li>

                        </ul>
                    </li>
                    <li><a href="/Content/Zeon/contact.html">İletişim</a></li>
                </ul>
                <div class="clear"></div>

            </div>
        </div>
    </div>
    
    <div class="content">
        @{ 
            Html.RenderAction("Slider", "Home");
        }
        

        @RenderBody()

       
      
        @{
            Html.RenderAction("Markalar", "Home");
        }
      
        
      
    </div>

    <div class="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-2 col-xs-6">
                    <ul class="socials">
                        <li><a href="/Content/Zeon/#"><img src="/Content/Zeon/images/elements/socials/facebook.png" alt="facebook" />Facebook</a></li>
                        <li><a href="/Content/Zeon/#"><img src="/Content/Zeon/images/elements/socials/twitter.png" alt="twitter" />Twitter</a></li>
                        <li><a href="/Content/Zeon/#"><img src="/Content/Zeon/images/elements/socials/youtube.png" alt="youtube" />Youtube</a></li>
                        <li><a href="/Content/Zeon/#"><img src="/Content/Zeon/images/elements/socials/googleplus.png" alt="google" />Google+</a></li>
                    </ul>
                </div>
                <div class="col-md-2 col-xs-6">
                    <ul>
                        <li><a href="/Content/Zeon/#">Register now</a></li>
                        <li><a href="/Content/Zeon/#">Subscribe for newsletter</a></li>
                        <li><a href="/Content/Zeon/#">Checkout</a></li>
